<configuration>
  <system.webServer>
    <staticContent>
      <mimeMap fileExtension=".json" mimeType="application/json"/>
      <mimeMap fileExtension=".woff" mimeType="application/font-woff"/>
      <mimeMap fileExtension=".woff2" mimeType="application/font-woff"/>
    </staticContent>
    <rewrite>
      <rules>
        <rule name="Imported Rule 1" stopProcessing="true">
          <match url="^index\.php$" ignoreCase="false"/>
          <action type="None"/>
        </rule>
        <rule name="Imported Rule 2" stopProcessing="true">
          <match url="^styles" ignoreCase="false"/>
          <action type="None"/>
        </rule>
        <rule name="Imported Rule 3" stopProcessing="true">
          <match url="^css" ignoreCase="false"/>
          <action type="None"/>
        </rule>
        <rule name="Imported Rule 4" stopProcessing="true">
          <match url="^js" ignoreCase="false"/>
          <action type="None"/>
        </rule>
        <rule name="Imported Rule 5" stopProcessing="true">
          <match url="^json" ignoreCase="false"/>
          <action type="None"/>
        </rule>
        <rule name="Imported Rule 6" stopProcessing="true">
          <match url="^javascript" ignoreCase="false"/>
          <action type="None"/>
        </rule>
        <rule name="Imported Rule 7" stopProcessing="true">
          <match url="^images" ignoreCase="false"/>
          <action type="None"/>
        </rule>
        <rule name="Imported Rule 8" stopProcessing="true">
          <match url="^temp" ignoreCase="false"/>
          <action type="None"/>
        </rule>
        <rule name="Imported Rule 9" stopProcessing="true">
          <match url="^csv" ignoreCase="false"/>
          <action type="None"/>
        </rule>
        <rule name="Imported Rule 10" stopProcessing="true">
          <match url="php$" ignoreCase="false"/>
          <action type="Rewrite" url="/index.php"/>
        </rule>
        <!--# RewriteCond %{REQUEST_FILENAME} !-d-->
        <rule name="Imported Rule 11" stopProcessing="true">
          <match url="." ignoreCase="false"/>
          <conditions>
            <add input="{REQUEST_FILENAME}" matchType="IsFile" ignoreCase="false" negate="true"/>
          </conditions>
          <action type="Rewrite" url="index.php"/>
        </rule>
        <!-- <rule name="Redirect HTTP to HTTPS">
          <match url="(.*)" />
          <conditions>
            <add input="{HTTPS}" pattern="off" ignoreCase="true" />
          </conditions>
          <action type="Redirect" url="https://{HTTP_HOST}/{R:1}" redirectType="Permanent"/>
        </rule> -->
      </rules>
    </rewrite>
  </system.webServer>

</configuration>
